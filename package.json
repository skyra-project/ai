{
	"name": "@skyra/ai",
	"version": "1.0.2",
	"description": "Advanced Gaming AI for Skyra",
	"author": "kyranet <kyradiscord@gmail.com>",
	"license": "MIT",
	"main": "dist/main.js",
	"module": "dist/main.mjs",
	"types": "dist/main.d.ts",
	"scripts": {
		"prepublishOnly": "yarn build",
		"test": "jest",
		"clean": "rimraf build dist",
		"build": "run-p build:node build:cpp",
		"build:cpp": "run-s gyp**",
		"build:node": "rollup -c rollup.config.ts",
		"gypconfigure": "node-gyp configure",
		"gypbuild": "node-gyp build",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz",
		"update": "yarn upgrade-interactive --latest",
		"install": "node-gyp rebuild"
	},
	"dependencies": {
		"bindings": "^1.5.0",
		"node-addon-api": "*"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.11.5",
		"@babel/preset-typescript": "^7.10.4",
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@sapphire/ts-config": "^1.0.1",
		"@types/bindings": "^1.3.0",
		"@types/jest": "^26.0.14",
		"@types/node": "^14.11.8",
		"cz-conventional-changelog": "^3.3.0",
		"husky": "^4.3.0",
		"jest": "^26.5.2",
		"jest-circus": "^26.5.2",
		"node-gyp": "^7.1.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.1.2",
		"pretty-quick": "^3.0.2",
		"rollup": "^2.29.0",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.27.3",
		"standard-version": "^9.0.0",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.3"
	},
	"files": [
		"src",
		"include",
		"binding.gyp",
		"dist",
		"!dist/.tsbuildinfo"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/ai.git"
	},
	"bugs": {
		"url": "https://github.com/skyra-project/skyra/issues"
	},
	"keywords": [
		"discord.js",
		"klasa",
		"bot",
		"standalone"
	],
	"engines": {
		"node": ">=12",
		"npm": ">=6"
	},
	"resolutions": {
		"chalk": "^4.1.0",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"kind-of": "^6.0.3",
		"lodash": "^4.17.20",
		"minimist": "^1.2.5"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn pretty-quick --staged"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"gypfile": true
}
