{
	"name": "@skyra/ai",
	"version": "1.2.0",
	"description": "Advanced Gaming AI for Skyra",
	"author": "kyranet <kyradiscord@gmail.com>",
	"license": "MIT",
	"main": "dist/main.js",
	"module": "dist/main.mjs",
	"types": "dist/main.d.ts",
	"sideEffects": false,
	"scripts": {
		"test": "yarn build && vitest run",
		"clean": "node scripts/clean.mjs",
		"build": "run-p \"build:node\" \"build:cpp\"",
		"build:cpp": "run-s \"gyp**\"",
		"build:node": "rollup -c rollup.config.mjs",
		"gypconfigure": "node-gyp configure",
		"gypbuild": "node-gyp build",
		"sversion": "standard-version",
		"update": "yarn up \"@*/*\" -i && yarn up \"*\" -i",
		"install": "node-gyp rebuild",
		"prepublishOnly": "yarn build",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"bindings": "^1.5.0",
		"node-addon-api": "*",
		"tslib": "^2.5.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-typescript": "^7.21.4",
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-conventional": "^17.6.1",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^4.0.0",
		"@types/bindings": "^1.5.1",
		"@types/node": "^18.15.11",
		"@vitest/coverage-c8": "^0.30.1",
		"cz-conventional-changelog": "^3.3.0",
		"esbuild": "^0.17.17",
		"husky": "^8.0.3",
		"node-gyp": "^9.3.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.7",
		"pretty-quick": "^3.1.3",
		"rollup": "^3.20.4",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.34.1",
		"standard-version": "^9.5.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4",
		"vite": "^4.2.1",
		"vitest": "^0.30.1"
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.8"
	},
	"files": [
		"src",
		"include",
		"binding.gyp",
		"dist",
		"!dist/.tsbuildinfo"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/ai.git"
	},
	"bugs": {
		"url": "https://github.com/skyra-project/skyra/issues"
	},
	"keywords": [
		"discord.js",
		"bot",
		"standalone"
	],
	"engines": {
		"node": ">=v14.18.0",
		"npm": ">=7.24.2"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"gypfile": true,
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.5.0"
}
