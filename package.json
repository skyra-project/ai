{
	"name": "@skyra/ai",
	"version": "1.0.2",
	"description": "Advanced Gaming AI for Skyra",
	"author": "kyranet <kyradiscord@gmail.com>",
	"license": "MIT",
	"main": "dist/main.js",
	"module": "dist/main.mjs",
	"types": "dist/main.d.ts",
	"scripts": {
		"test": "jest",
		"clean": "rimraf build dist",
		"build": "run-p build:node build:cpp",
		"build:cpp": "run-s gyp**",
		"build:node": "rollup -c rollup.config.ts",
		"gypconfigure": "node-gyp configure",
		"gypbuild": "node-gyp build",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz",
		"update": "yarn upgrade-interactive --latest",
		"install": "node-gyp rebuild",
		"prepublishOnly": "yarn build && pinst --disable",
		"postinstall": "husky install .github/husky",
		"postpublish": "pinst --enable"
	},
	"dependencies": {
		"bindings": "^1.5.0",
		"node-addon-api": "*"
	},
	"devDependencies": {
		"@babel/core": "^7.12.13",
		"@babel/preset-env": "^7.12.13",
		"@babel/preset-typescript": "^7.12.13",
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@sapphire/prettier-config": "^1.0.6",
		"@sapphire/ts-config": "^2.0.0",
		"@types/bindings": "^1.5.0",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.25",
		"cz-conventional-changelog": "^3.3.0",
		"husky": "^5.0.9",
		"jest": "^26.6.3",
		"jest-circus": "^26.6.3",
		"node-gyp": "^7.1.2",
		"npm-run-all": "^4.1.5",
		"pinst": "^2.1.4",
		"prettier": "^2.2.1",
		"pretty-quick": "^3.1.0",
		"rollup": "^2.38.5",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.29.0",
		"standard-version": "^9.1.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.5"
	},
	"files": [
		"src",
		"include",
		"binding.gyp",
		"dist",
		"!dist/.tsbuildinfo"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/ai.git"
	},
	"bugs": {
		"url": "https://github.com/skyra-project/skyra/issues"
	},
	"keywords": [
		"discord.js",
		"bot",
		"standalone"
	],
	"engines": {
		"node": ">=12",
		"npm": ">=6"
	},
	"resolutions": {
		"chalk": "^4.1.0",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"kind-of": "^6.0.3",
		"lodash": "^4.17.20",
		"minimist": "^1.2.5"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"gypfile": true,
	"prettier": "@sapphire/prettier-config"
}
